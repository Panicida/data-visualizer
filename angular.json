{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
      "frontend": {
        "projectType": "application",
        "schematics": {
          "@schematics/angular:application": {
            "strict": true
          },
          "@schematics/angular:component": {
            "styleext": "scss"
          }
        },
        "root": "",
        "sourceRoot": "src",
        "prefix": "app",
        "architect": {
          "build": {
            "builder": "@angular-devkit/build-angular:browser",
            "options": {
              "outputPath": "dist/frontend",
              "index": "src/frontend/index.html",
              "main": "src/frontend/main.ts",
              "polyfills": "src/frontend/polyfills.ts",
              "tsConfig": "src/frontend/tsconfig.app.json",
              "aot": true,
              "assets": [
                "src/frontend/favicon.ico",
                "src/frontend/assets"
              ],
              "styles": [
                "src/frontend/styles/styles.scss"
              ],
              "scripts": []
            },
            "configurations": {
              "production": {
                "fileReplacements": [
                  {
                    "replace": "src/frontend/environments/environment.ts",
                    "with": "src/frontend/environments/environment.prod.ts"
                  }
                ],
                "optimization": true,
                "outputHashing": "all",
                "sourceMap": false,
                "namedChunks": false,
                "extractLicenses": true,
                "vendorChunk": false,
                "buildOptimizer": true,
                "budgets": [
                  {
                    "type": "initial",
                    "maximumWarning": "500kb",
                    "maximumError": "1mb"
                  },
                  {
                    "type": "anyComponentStyle",
                    "maximumWarning": "2kb",
                    "maximumError": "4kb"
                  }
                ]
              },
              "development": {
                "optimization": false,
                "outputHashing": "all",
                "sourceMap": true,
                "namedChunks": false,
                "aot": false,
                "extractLicenses": true,
                "vendorChunk": false,
                "buildOptimizer": false
              }
            }
          },
          "test": {
            "builder": "@angular-devkit/build-angular:karma",
            "options": {
              "main": "src/frontend/test.ts",
              "polyfills": "src/frontend/polyfills.ts",
              "tsConfig": "src/frontend/tsconfig.spec.json",
              "karmaConfig": "karma.conf.js",
              "assets": [
                "src/frontend/favicon.ico",
                "src/frontend/assets"
              ],
              "styles": [
                "src/frontend/styles/styles.scss"
              ],
              "scripts": []
            }
          }
        }
      }
    },
    "defaultProject": "frontend"
  }
  